<h1>Welcome, <%= @user.username %></h1>
<br>
<% @user_goals.each_with_index do |goal,index| %>
    <h2><b>Goal #<%= index + 1 %>: </b><%= goal.goal_name %> <a href='/goals/<%= goal.id %>'>Click to edit or delete</a></h2>
    <p><b>Goal description: </b><%= goal.description %></p>
    <p><b>Has this been achieved yet? </b><%= goal.achieved? %></p>
<% end %>

